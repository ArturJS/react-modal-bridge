{
  "name": "@ajs/react-modal",
  "version": "0.0.1",
  "description": "The bridge between lawless react-modal and strict opinionated modal API",
  "main": "build/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build-test": "webpack --mode development",
    "e2e": "docker-compose --file docker-compose.e2e.yml up --abort-on-container-exit",
    "docker-rebuild-e2e-tests": "docker-compose --file docker-compose.e2e.yml up --no-deps --build e2e-tests",
    "lint": "npm run lint-js && npm run lint-scss",
    "lint-fix": "npm run lint-js -- --fix && npm run lint-scss -- --fix",
    "lint-js": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "lint-scss": "stylelint 'src/**/*.scss'",
    "lint-staged": "lint-staged",
    "test": "jest --env=jsdom",
    "test-coverage": "jest --coverage --env=jsdom",
    "test-watch": "jest --watch --env=jsdom",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArturJS/react-modal-bridge.git"
  },
  "keywords": [
    "react",
    "modal"
  ],
  "author": "ArturJS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArturJS/react-modal-bridge/issues"
  },
  "homepage": "https://github.com/ArturJS/react-modal-bridge#readme",
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-modal": "^3.8.1",
    "react-transition-group": "^2.6.0",
    "recompact": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "@emotion/core": "^10.0.7",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.4",
    "sass-loader": "^7.1.0",
    "string-replace-loader": "^2.1.1",
    "stylelint": "^9.10.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-order": "^2.1.0",
    "stylelint-scss": "^3.5.4",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "jest --bail --findRelatedTests --env=jsdom",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "npm run lint-staged"
    }
  }
}
